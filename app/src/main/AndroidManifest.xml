<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_tensorflow"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_tensorflow"
        android:supportsRtl="true"
        android:theme="@style/Theme.MLProjects">
        <activity
            android:name=".walking.RunningWalkingClassifierActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".animals.AnimalsActivity"
            android:exported="true" />
        <activity
            android:name=".content_recommendation.RecommendationsActivity"
            android:exported="true"
            android:theme="@style/Theme.MLProjects.NoActionBar" />
    </application>

</manifest>